mongo --port 27010 --eval 'rs.initiate({_id: "test", configsvr: true, members: [{ _id : 0, host : "localhost:27010" }]})'
sudo mongod --shardsvr  --bind_ip localhost --replSet shards --dbpath /data/mongo_node1 --port 27018 --fork --logpath /data/node1.log 
